{
  "expo": {
    "name": "expo-project",
    "slug": "expo-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/splash_image.jpg",
    "splash": {
      "image": "./app/assets/splash_image.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/splash_image.jpg"
    },
    "android": {
      "package": "com.ambuj.ideata.expoproject"
    }
  }
}
